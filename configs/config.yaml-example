plugins:
  prompt-manager:
    openai:
      base_url: https://api.openai.com/v1
      api_key: sk-proj-your-openai-api-key
      model: gpt-4o
      temperature: 0.5
    max_prompts: 50
  telegram:
    bot_token: your-telegram-bot-token
    bot_name: your-telegram-bot-name
    allowed_chat_ids:
      - 1234567890
      - 9876543210
  clock:
  calculator:
  http:
  short-term-memory:
    max_messages: 10
    max_length: 140
    period: 6
