plugins:
  prompt-manager:
    openai:
      base_url: https://api.openai.com/v1
      api_key: sk-proj-your-openai-api-key
      model: gpt-4o
      temperature: 0.5
    max_prompts: 50
  telegram:
    bot_token: your-telegram-bot-token
    bot_name: your-telegram-bot-name
    allowed_chat_ids:
      - 1234567890
      - 9876543210
  clock:
  calculator:
  http:
  short-term-memory:
    max_messages: 50
    max_length: 1000
  llm:
    base_url: https://api.openai.com/v1
    api_key: sk-proj-your-openai-api-key
    models:
      chat:
        - name: gpt-4o
          desc: GPT-4o is good at general purpose tasks.
      image:
        - name: dall-e-3
          desc: DALL-E 3 is good at generating images. Whenever you are requested to generate images, use this model.
  file-system:
  python:
  long-term-memory:
  discord:
    bot_token: your-discord-bot-token
    allowed_channel_ids:
      - "1234567890"
      - "9876543210"
