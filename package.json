  },
  "homepage": "https://athenalab.ai/",
  "description": "",
  "dependencies": {
    "amadeus": "^11.0.0",
    "discord.js": "^14.17.3",
    "follow-redirects": "^1.15.9",
    "html-to-text": "^9.0.5",
    "image2uri": "^2.1.2",
    "istextorbinary": "^9.5.0",
    "node-telegram-bot-api": "^0.63.0",
    "openai": "^4.78.1",
    "python-shell": "^5.0.0",
    "winston": "^3.17.0",
    "yaml": "^2.7.0"
  },
